<div class="container" style="background-color:rgb(154, 163, 173); width: 600px; height: auto; padding-bottom: 10px;">
    <h3>Centro de mensajes</h3>
    <br>
    <hr size="5px">
    <br>
    <div id="chat">
        <% msj.forEach(msj=>{ %>
        <div>
            <span style="color:blue; font-weight: bold;"><%= msj.email %></span>
            <span style="color:brown"><%= msj.timestamp %></span>
            <span style="color:green; font-style: italic;"><%= msj.texto %></span>
        </div>
        <% }) %>
    </div>
    <hr size="5px">
</div>